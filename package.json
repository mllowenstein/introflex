{
  "name": "introflex",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "ng build",
    "docs:serve": "compodoc -s",
    "buildx": "npm run write:env -s && ng build",
    "startx": "npm run write:env -s && ng serve --proxy-config proxy.conf.js",
    "servex": "npm run build -s && npx http-server ./dist -p 4200",
    "local": "ng serve --configuration local --port 4444",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "start": "ng serve --port 4444 --host 0.0.0.0",
    "app:format": "npx prettier --write .",
    "app:scan": "npx prettier --check .",
    "app:prettify": "prettier --write \"src/**/*.ts\"",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort",
    "docs:build": "compodoc -p tsconfig.doc.json",
    "docs:build-and-serve": "compodoc -p tsconfig.doc.json -s",
    "write:env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./src/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./src/**/*.{ts,js,html,scss}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "npx lint-staged",
    "prepare": "npx simple-git-hooks",
    "generate": "ng generate"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.0",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.0",
    "@angular/compiler": "^17.0.0",
    "@angular/core": "^17.0.0",
    "@angular/fire": "^17.0.0",
    "@angular/forms": "^17.0.0",
    "@angular/material": "^17.0.4",
    "@angular/platform-browser": "^17.0.0",
    "@angular/platform-browser-dynamic": "^17.0.0",
    "@angular/router": "^17.0.0",
    "@apollo/client": "^3.8.8",
    "apollo-angular": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "date-fns": "^3.0.4",
    "firebase": "^10.7.1",
    "graphql": "^16.8.1",
    "graphql-tag": "^2.12.6",
    "hammerjs": "^2.0.8",
    "material-design-icons": "^3.0.1",
    "material-design-icons-iconfont": "^6.7.0",
    "moment": "^2.29.4",
    "postcss-scss": "^4.0.9",
    "primeicons": "^6.0.1",
    "primeng": "^17.2.0",
    "qs": "^6.11.2",
    "rxjs": "~7.8.0",
    "save": "^2.9.0",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.7.2",
    "tailwindcss": "^3.4.0",
    "three": "^0.159.0",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "web3": "^4.3.0",
    "ws": "^8.15.1",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.7",
    "@angular/cli": "^17.0.7",
    "@angular/compiler-cli": "^17.0.0",
    "@iconify/json": "^2.2.160",
    "@iconify/tailwind": "^0.1.4",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/jasmine": "~5.1.0",
    "cypress": "^13.6.1",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.0",
    "markdownlint": "^0.32.1",
    "prettier": "^3.1.1",
    "prettier-eslint": "^16.2.0",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "simple-git-hooks": "^2.9.0",
    "tslint": "^6.1.3",
    "typescript": "~5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
